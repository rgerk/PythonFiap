nome = input("Informe o nome: ")
idade = int(input("Informe a idade: "))
risco = input("Grupo de Risco sim ou nao: ")
if risco != "SIM" or risco != "NAO":
    risco = input("Grupo de Risco sim ou nao: ").upper()
 
elif idade < 15:
    print(nome.upper(), " Prioritario")   
elif idade > 60:    
     print(nome.upper(), " Prioritario") 
elif risco == 'S':
    print("Prioritario")      
else:
    print("Segue a fila")   
